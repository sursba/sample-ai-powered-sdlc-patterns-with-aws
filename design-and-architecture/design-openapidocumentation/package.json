{
  "name": "domain-driven-api-generator",
  "version": "1.0.0",
  "description": "A multi-stage application for generating OpenAPI specifications from domain models",
  "main": "backend-lambda/server/index.js",
  "scripts": {
    "start": "node start-server.js",
    "start:server": "node backend-lambda/server/index.js",
    "dev": "nodemon backend-lambda/server/index.js",
    "server": "nodemon backend-lambda/server/index.js",
    "client": "cd client && npm run start",
    "dev:all": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.474.0",
    "@aws-sdk/client-lambda": "^3.833.0",
    "@aws-sdk/credential-provider-node": "^3.474.0",
    "@aws-sdk/credential-providers": "^3.840.0",
    "@aws-sdk/hash-node": "^3.0.0",
    "@aws-sdk/protocol-http": "^3.370.0",
    "@aws-sdk/signature-v4": "^3.370.0",
    "@smithy/node-http-handler": "^2.1.10",
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "serverless-offline": "^14.4.0"
  }
}
